<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.book">

	<resultMap id="book"
		type="com.basic.projectbook.vo.BookVO">
		<id property="no" column="no" />
		<result property="name" column="name" />
		<result property="price" column="price" />
		<result property="genre" column="genre" />
		<result property="qty" column="qty" />
		<result property="store1" column="store1" />
		<result property="store2" column="store2" />
		<result property="store3" column="store3" />
		<result property="country" column="country" />
		<result property="sale" column="sale" />
		<result property="img" column="img" />
		<result property="writer" column="writer" />
		<result property="info" column="info" />
		<result property="publication" column="publication" />
		<result property="company" column="company" />
		<result property="discount" column="discount" />
	</resultMap>

	<select id="getAllBook" resultMap="book">
		select * from book
	</select>

	

</mapper>